%form#new_contact
  %div.actions
    %fieldset
      %p
        %label{for: "contact_lastname"} = "Lastname"
        %br
        %input{type: "text", id: "contact_lastname", name: "contact[lastname]"}

      %p
        %label{for: "contact_firstname"} = "Firstname"
        %br
        %input{type: "text", id: "contact_firstname", name: "contact[firstname]"}

      %p
        %label{for: "contact_email"} = "Email"
        %br
        %input{type: "text", id: "contact_email", name: "contact[email]"}

      %p
        %label{for: "contact_tel_home"} = "Phone tel (home)"
        %br
        %input{type: "tel", id: "contact_tel_home", name: "contact[tel_home]"}

      %p
        %label{for: "contact_tel_mobil"} = "Phone tel (mobil)"
        %br
        %input{type: "tel", id: "contact_tel_mobil", name: "contact[tel_mobil]"}

      %p
        %label{for: "contact_adress"} = "Adress"
        %br
        %input{type: "text", id: "contact_adress" , name: "contact[adress]"}

      %p
        %label{for: "contact_country"} = "Country"
        %br
        %input{type: "text", id: "contact_country", name: "contact[country]"}

    %p
      %input{type: "submit", value: "Submit"}
