%table{border: 1}
  %tr
    %th
      = translate.contacts.index.assoc_contact
    %th
      = translate.contacts.index.lastname
    %th
      = translate.contacts.index.firstname
    %th
      = translate.contacts.index.edit
    %th
      = translate.contacts.index.destroy
  :each contact in contacts
    %tr.contact_lists{id: "contact_lists_"+contact.id}
      %td
        = "checkbox" 
      %td
        = contact.lastname
      %td
        = contact.firstname
