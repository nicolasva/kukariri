%table.table.tablesorter
  %thead
    %tr
      %th
        = translate.contacts.index.assoc_contact
      %th
        = translate.activerecord.attributes.contact.lastname
      %th
        = translate.activerecord.attributes.contact.firstname
      %th
        = translate.contacts.index.edit
      %th
        = translate.contacts.index.destroy
  :each contact in contacts
    %tbody
      %tr.contact_lists{id: "contact_lists_"+contact.id}
        %td
          = "checkbox" 
        %td
          = contact.lastname
        %td
          = contact.firstname
%ul
%script{type: "text/javascript", src: "/assets/rails/items.js"}
