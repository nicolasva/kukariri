    %input{type: "text", id: "item_title", name: "item[title]"}

  %p
    %label{for: "item_pictures_attributes_0_pictures"} = "Pictures"
    %br
    %input{type: "file",  id: "item_pictures_attributes_0_pictures", name: "item[pictures_attributes][0][picture]"}

  %p
    %label{for: "item_pictures_attributes_0_pictures_attributes_0_types_attributes_0_descriptif"} = "descriptif"
